<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.juniorsantoniomedinalandeon.juniorschat">
    // Tengo que agregar el permiso de acceso a Internet
<uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_mi_icono"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_mi_icono"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChatActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Programacion"></activity>
        <activity android:name=".AcercaDe"
                  android:label="@string/acerca_de"
                  android:theme="@android:style/Theme.Holo.Dialog"/>
    </application>

</manifest>